<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App4.MenuP.MasterPage"
             Title="Master" 
             BackgroundColor ="Black">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" VerticalOptions="CenterAndExpand" >
        <ListView x:Name="listView" VerticalOptions="FillAndExpand" RowHeight="50"  HorizontalOptions="CenterAndExpand"  SeparatorVisibility="None">
            <ListView.ItemTemplate>
                <DataTemplate>

                        <ImageCell Text="{Binding Title}" Height="100" TextColor="White" />
                       



                    </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
    </ContentPage.Content>
</ContentPage>